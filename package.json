  "scripts": {
    "critical": "critical dist/index.html -b dist/ -w 1300 -h 900 -i true -H dist/index.html",
    "ghpages:": "gh-pages -d ./dist",
    "surge": "surge dist --domain $npm_package_name.surge.sh",
    "analyze": "webpack --config _sys/webpack.production.js --profile --json > stats.json",
    "prebuild": "rm -rf ./dist",
    "build": "cross-env NODE_ENV=production webpack --config _sys/webpack.production.js",
    "postbuild": "npm run critical",
    "build:publish": "npm run build && npm run surge",
    "postbuild:publish": "rm -rf ./dist",
    "lint:js": "eslint ./app",
    "start": "webpack-dev-server --config _sys/webpack.config.js --no-info --hot --inline --content-base ./public --port 4000",
    "test": "npm run test:watch -- --single-run",
    "test:ci": "cross-env NODE_ENV=test karma start _sys/karma.ci.conf.js --single-run",
    "test:coverage": "cross-env NODE_ENV=test karma start _sys/karma.coverage.conf.js --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start _sys/karma.conf.js"
  }
